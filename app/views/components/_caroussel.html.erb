 <div class="container-carousel">
    <div id="carouselExampleAutoplaying" class="carousel slide" data-bs-ride="carousel">
      <div class="carousel-inner" style="filter: grayscale(0%)">
        <div class="carousel-item active carousel-picture">
          <%= link_to pokemon_path(Art.find(4)) do %>
          <%= image_tag Art.find(4).photo, class: "d-block w-100", alt: "..." %>
          <% end %>
          <p style="text-align: center;">
            <a class="btn btn-ghost-carousel" data-bs-toggle="collapse" href="#collapseExample" role="button" aria-expanded="false" aria-controls="collapseExample">
              Interested?
            </a>
          </p>
          <div class="collapse" id="collapseExample">
            <%# <div class="card card-body" style="text-align: center;"> %>
            <div style="text-align: center; margin-top: 30px; margin-bottom: 30px">
              <%= link_to "<strong><i>#{Art.find(4).title}</strong></i> <br>is available!".html_safe, art_path(Art.find(4)), class: "paragraph-carousel" %>
            </div>
          </div>
        </div>

